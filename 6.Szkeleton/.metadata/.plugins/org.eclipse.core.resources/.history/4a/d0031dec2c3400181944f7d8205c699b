package server;

import java.io.ObjectInputStream;

public class SzerverOlvaso {

	ObjectInputStream in;
	Kapcsolat c;
	boolean alive;
	int id=0;
	Szerver server;
	
	public SzerverOlvaso(ObjectInputStream ois, Kapcsolat c, Szerver server) 
	{
		alive=true;
		in = ois;
		this.c=c;
		this.server=server;		
	}

}
