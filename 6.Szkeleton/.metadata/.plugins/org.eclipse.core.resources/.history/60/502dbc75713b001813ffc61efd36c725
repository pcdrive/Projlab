package server;

import java.util.LinkedList;

import javafx.util.Pair;
import sokoban.Iranyi;

public class FIFO {
	
	LinkedList<Pair<Iranyi, String>> commands; 
	
	public FIFO() 
	{
		commands = new LinkedList<Pair<Iranyi, String>>();
	}
	
	public void push(Iranyi i, String nev) {commands.add(new Pair<Iranyi, String>(i,nev));}
	
	public Pair<Iranyi, String> pull() {return commands.removeFirst();}
	
	public boolean isEmpty() {return commands.size()==0;}

}
